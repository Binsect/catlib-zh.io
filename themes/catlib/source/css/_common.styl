@import "_settings"

body
    font-family $body-font
    font-size $body-font-size
    -webkit-font-smoothing antialiased
    -moz-osx-font-smoothing grayscale
    color $dark
    background-color white
    margin 0
    &.docs
        padding-top: $header-height

h1,h2,p
    padding 0;
    margin 0;

a
    text-decoration none

ul
    list-style none
